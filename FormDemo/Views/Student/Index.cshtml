@model StudentModel

<!-- General Form Elements -->
<h1>Student Regstration</h1>
<form asp-controller="Student" asp-action="Save">
    <div class="row mb-3">
        <label for="inputNumber" asp-for="StudentName" class="col-sm-4 col-form-label">StudentName</label>
        <div class="col-sm-10">
            <input type="text" asp-for="StudentName" class="form-control">
            <span asp-validation-for="StudentName" class="text-danger"></span>
        </div>
    </div>
    <div class="row mb-3">
        <label for="inputNumber" class="col-sm-4 col-form-label">EnrollmentNumber</label>
        <div class="col-sm-10">
            <input type="text" class="form-control">
            <span asp-validation-for="EnrollmentNo" class="text-danger"></span>
        </div>
    </div>

    <div class="row mb-3">
        <label for="inputText" class="col-sm-4 col-form-label">Password</label>
        <div class="col-sm-10">
            <input type="text" class="form-control">
            <span asp-validation-for="Password" class="text-danger"></span>
        </div>
    </div>

    <div class="row mb-3">
        <label for="email" class="col-sm-4 col-form-label">EmailInstitute</label>
        <div class="col-sm-10">
            <input type="email" class="form-control">
            <span asp-validation-for="EmailInstitute" class="text-danger"></span>
        </div>
    </div>

    <div class="row mb-3">
        <label for="inputText" class="col-sm-4 col-form-label">ContactNo</label>
        <div class="col-sm-10">
            <input type="number" class="form-control">
            <span asp-validation-for="ContactNo" class="text-danger"></span>
        </div>
    </div>

   



    <div class="row mb-3">
        
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary" >Submit </button>
        </div>
    </div>

</form><!-- End General Form Elements -->

